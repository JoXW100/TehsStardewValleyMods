{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "TrashPack",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "TrashEntries"
  ],
  "properties": {
    "$schema": {
      "type": [
        "null",
        "string"
      ],
      "description": "Optional schema URL."
    },
    "TrashEntries": {
      "type": "array",
      "description": "The trash entries to add.",
      "items": {
        "$ref": "#/definitions/TrashEntry"
      }
    }
  },
  "definitions": {
    "TrashEntry": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "AvailabilityInfo",
        "ItemKey"
      ],
      "properties": {
        "AvailabilityInfo": {
          "description": "The availability information.",
          "oneOf": [
            {
              "$ref": "#/definitions/AvailabilityInfo"
            }
          ]
        },
        "ItemKey": {
          "type": "string",
          "description": "The item key."
        }
      }
    },
    "AvailabilityInfo": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "BaseChance"
      ],
      "properties": {
        "BaseChance": {
          "type": "number",
          "description": "The base chance this will be caught. This is not a percentage chance, but rather a weight relative to all available entries.",
          "format": "double"
        },
        "WaterTypes": {
          "type": "array",
          "description": "The type of water this can be caught in. Each location handles this differently. Default is all.",
          "items": {
            "$ref": "#/definitions/WaterTypes"
          }
        },
        "MinFishingLevel": {
          "type": "integer",
          "description": "Required fishing level to see this.",
          "format": "int32",
          "default": 0
        },
        "MaxFishingLevel": {
          "type": [
            "integer",
            "null"
          ],
          "description": "Maximum fishing level required to see this, or null for no max.",
          "format": "int32"
        },
        "IncludeLocations": {
          "type": "array",
          "description": "List of locations this should be available in. Leaving this empty will make this available everywhere. Some locations have special handling. For example, the mines use the location names 'UndergroundMine' and 'UndergroundMine/N', where N is the floor number (both location names are valid for the floor).",
          "items": {
            "type": "string"
          }
        },
        "ExcludeLocations": {
          "type": "array",
          "description": "List of locations this should not be available in. This takes priority over IncludeLocations.",
          "items": {
            "type": "string"
          }
        },
        "When": {
          "type": "object",
          "description": "Content Patcher conditions for when this is available.",
          "additionalProperties": {
            "type": "string"
          }
        }
      }
    },
    "WaterTypes": {
      "type": "string",
      "description": "",
      "x-enumFlags": true,
      "x-enumNames": [
        "None",
        "River",
        "PondOrOcean",
        "Freshwater",
        "All"
      ],
      "enum": [
        "None",
        "River",
        "PondOrOcean",
        "Freshwater",
        "All"
      ]
    }
  }
}