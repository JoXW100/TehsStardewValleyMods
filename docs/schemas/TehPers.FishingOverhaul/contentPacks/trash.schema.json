{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "TrashPack",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "TrashEntries"
  ],
  "properties": {
    "$schema": {
      "type": [
        "null",
        "string"
      ]
    },
    "TrashEntries": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/TrashEntry"
      }
    }
  },
  "definitions": {
    "TrashEntry": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "ItemKey": {
          "type": "string",
          "description": "The item key."
        },
        "Availability": {
          "description": "The availability information.",
          "oneOf": [
            {
              "$ref": "#/definitions/Availability"
            }
          ]
        }
      }
    },
    "Availability": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "BaseChance"
      ],
      "properties": {
        "BaseChance": {
          "type": "number",
          "description": "The base chance this will be caught. This is not a percentage chance, but rather a weight relative to all available entries.",
          "format": "double"
        },
        "StartTime": {
          "type": "integer",
          "description": "Time this becomes available (inclusive).",
          "format": "int32",
          "default": 600
        },
        "EndTime": {
          "type": "integer",
          "description": "Time this is no longer available (exclusive).",
          "format": "int32",
          "default": 2600
        },
        "Seasons": {
          "type": "array",
          "description": "Seasons this can be caught in. Default is all.",
          "items": {
            "$ref": "#/definitions/Seasons"
          }
        },
        "Weathers": {
          "type": "array",
          "description": "Weathers this can be caught in. Default is all.",
          "items": {
            "$ref": "#/definitions/Weathers"
          }
        },
        "WaterTypes": {
          "type": "array",
          "description": "The type of water this can be caught in. Each location handles this differently. Default is all.",
          "items": {
            "$ref": "#/definitions/WaterTypes"
          }
        },
        "MinFishingLevel": {
          "type": "integer",
          "description": "Required fishing level to see this.",
          "format": "int32",
          "default": 0
        },
        "MaxFishingLevel": {
          "type": [
            "integer",
            "null"
          ],
          "description": "Maximum fishing level required to see this, or null for no max.",
          "format": "int32"
        },
        "IncludeLocations": {
          "type": "array",
          "description": "List of locations this should be available in. Leaving this empty will make this available everywhere. Some locations have special handling. For example, the mines use the location names 'UndergroundMine' and 'UndergroundMine#N', where N is the floor number (both location names are valid for the floor).",
          "items": {
            "type": "string"
          }
        },
        "ExcludeLocations": {
          "type": "array",
          "description": "List of locations this should not be available in. This takes priority over IncludeLocations.",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "Seasons": {
      "type": "string",
      "description": "",
      "x-enumFlags": true,
      "x-enumNames": [
        "None",
        "Spring",
        "Summer",
        "Fall",
        "Winter",
        "All"
      ],
      "enum": [
        "None",
        "Spring",
        "Summer",
        "Fall",
        "Winter",
        "All"
      ]
    },
    "Weathers": {
      "type": "string",
      "description": "",
      "x-enumFlags": true,
      "x-enumNames": [
        "None",
        "Sunny",
        "Rainy",
        "All"
      ],
      "enum": [
        "None",
        "Sunny",
        "Rainy",
        "All"
      ]
    },
    "WaterTypes": {
      "type": "string",
      "description": "",
      "x-enumFlags": true,
      "x-enumNames": [
        "None",
        "Ocean",
        "River",
        "Pond",
        "Freshwater",
        "All"
      ],
      "enum": [
        "None",
        "River",
        "River",
        "Pond",
        "Freshwater",
        "All"
      ]
    }
  }
}